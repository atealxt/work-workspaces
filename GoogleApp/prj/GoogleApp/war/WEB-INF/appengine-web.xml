<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

	<application>atealxt</application>
	<version>1</version>

	<sessions-enabled>true</sessions-enabled>

    <static-files>
        <include path="favicon.ico" />
        <include path="images/**.png" />
        <include path="images/**.jpg" />
        <include path="images/**.gif" />
        <include path="javascripts/**.js" />
        <include path="stylesheets/**.css" />
        <include path="syntaxhighlighter/**.*" />
        <include path="tiny_mce/**.*" />
    </static-files>

	<precompilation-enabled>true</precompilation-enabled>

</appengine-web-app>