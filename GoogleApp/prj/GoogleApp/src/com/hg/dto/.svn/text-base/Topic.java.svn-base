package com.hg.dto;

import java.util.Date;

public class Topic {

    private String id;
    private String alias;
    private String title;
    private String summary;
    private boolean postBySummary;
    private String content;
    private Date createTime;
    private String createby;
    private int readCnt;
    private int commentCnt;
    private String type;
    private String ip;

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getAlias() {
        return alias;
    }

    public void setAlias(String alias) {
        this.alias = alias;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public int getReadCnt() {
        return readCnt;
    }

    public void setReadCnt(int readCnt) {
        this.readCnt = readCnt;
    }

    public int getCommentCnt() {
        return commentCnt;
    }

    public void setCommentCnt(int commentCnt) {
        this.commentCnt = commentCnt;
    }

    public String getCreateby() {
        return createby;
    }

    public void setCreateby(String createby) {
        this.createby = createby;
    }

    public String getContent() {
        return content;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public String getSummary() {
        return summary;
    }

    public void setSummary(String summary) {
        this.summary = summary;
    }

    public boolean isPostBySummary() {
        return postBySummary;
    }

    public void setPostBySummary(boolean postBySummary) {
        this.postBySummary = postBySummary;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getIp() {
        return ip;
    }

    public void setIp(String ip) {
        this.ip = ip;
    }

    @Override
    public String toString() {
        return "Topic [alias=" + alias + ", commentCnt=" + commentCnt + ", content=" + content + ", createTime="
                + createTime + ", createby=" + createby + ", id=" + id + ", ip=" + ip + ", postBySummary="
                + postBySummary + ", readCnt=" + readCnt + ", summary=" + summary + ", title=" + title + ", type="
                + type + "]";
    }

}
