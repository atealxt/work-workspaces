<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.m2.entity">
  <class name="Org" table="M2_Org">
    <id name="id" column="ID" type="java.lang.Integer">
      <generator class="native"></generator>
    </id>
    <property name="name" column="NAME" type="java.lang.String" length="50"/>
    <property name="parentId" column="PARENT_ID" type="java.lang.Integer" />
    <property name="description" column="DESCRIPTION" type="java.lang.String" length="100"/>
    
  </class>
</hibernate-mapping>