<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx.xsd        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd">  
  <bean id="albumDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.IAlbumDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.Album</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="albumCategoryDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.IAlbumCategoryDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.AlbumCategory</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="blogDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.IBlogDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.Blog</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="commentDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.ICommentDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.Comment</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="linkDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.ILinkDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.Link</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="topicDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.ITopicDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.Topic</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="topicCategoryDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.ITopicCategoryDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.TopicCategory</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  
  <bean id="userDao" parent="abstractDao"> 
    <property name="proxyInterfaces"> 
      <value>com.easyjf.simpleblog.dao.IUserDAO</value> 
    </property>  
    <property name="target"> 
      <bean parent="baseDAO"> 
        <constructor-arg> 
          <value>com.easyjf.simpleblog.domain.User</value> 
        </constructor-arg> 
      </bean> 
    </property> 
  </bean>  

</beans>
