		<display-name>CheckLogin</display-name>
		<servlet-name>CheckLogin</servlet-name>
		<servlet-class>com.soa.servlet.CheckLogin</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>CollaborationCategory.do</display-name>
		<servlet-name>CollaborationCategory.do</servlet-name>
		<servlet-class>com.soa.servlet.CollaborationCategory</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>StateSet.do</display-name>
		<servlet-name>StateSet.do</servlet-name>
		<servlet-class>com.soa.servlet.StateSet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>FileUpload.do</display-name>
		<servlet-name>FileUpload.do</servlet-name>
		<servlet-class>com.soa.servlet.FileUpload</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>BaseData.do</display-name>
		<servlet-name>BaseData.do</servlet-name>
		<servlet-class>com.soa.base.BaseData</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>RequestInfo.do</display-name>
		<servlet-name>RequestInfo.do</servlet-name>
		<servlet-class>com.soa.servlet.RequestInfo</servlet-class>
	</servlet>
	<servlet>
