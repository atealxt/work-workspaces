<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.herograve.user.Userlevelshow" table="userlevelshow">
                                   
		<id name="id" type="java.lang.Integer" column="levelshow_id">
                    <generator class="increment" />
		</id>
                                
		<property name="name" type="java.lang.String" column="levelshow_name" length="16" />
	</class>
</hibernate-mapping>