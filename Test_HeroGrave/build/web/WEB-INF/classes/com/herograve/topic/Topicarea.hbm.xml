<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.herograve.topic.Topicarea" table="topicarea">
                                   
		<id name="id" type="java.lang.Integer" column="topicarea_id">
                    <generator class="increment" />
		</id>
                                
		<property name="name" type="java.lang.String" column="topicarea_name" length="16" />
		<property name="level" type="java.lang.Integer" column="topicarea_level"  />
	</class>
</hibernate-mapping>