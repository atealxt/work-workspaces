<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.py.hib.mytest.criteria.CriteriaTest" table="criteria_test">
    <id column="id" length="32" name="id" type="java.lang.Integer">
      <generator class="increment"/>
    </id>
    <property column="name" length="32" name="name" not-null="true" type="java.lang.String"/>
    <property column="price" length="32" name="price" not-null="true" type="java.lang.Integer"/>
  </class>
</hibernate-mapping>
