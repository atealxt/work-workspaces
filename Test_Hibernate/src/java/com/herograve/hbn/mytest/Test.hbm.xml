<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.herograve.hbn.mytest.Test" table="TEST">
    <id column="TEST_ID" length="32" name="id" type="java.lang.String">
      <generator class="uuid"/>
    </id>
    <property column="TEST_NAME" length="32" name="name" type="java.lang.String"/>
    <property column="TEST_TYPE" name="type" type="java.lang.Integer"/>
    <property column="TEST_CHAR" length="10" name="memo" type="java.lang.String"/>
  </class>
</hibernate-mapping>
